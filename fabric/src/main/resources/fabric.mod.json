{
    "schemaVersion": 1,
    "id": "${id}",
    "version": "${version}",
    "environment": "*",
    "entrypoints": {
        "main": [
          "org.wallentines.mcore.Init"
        ],
        "client": [
            "org.wallentines.mcore.InitClient"
        ]
    },
    "depends": {
        "fabricloader": "*",
        "fabric-api-base": "*"
    },
    "mixins": [
        "midnightcore.mixins.json"
    ],
    "custom": {
        "loom:injected_interfaces": {
            "net/minecraft/server/MinecraftServer": [ "org/wallentines/mcore/Server" ],
            "net/minecraft/class_3222": [ "org/wallentines/mcore/Player", "org/wallentines/mcore/ScoreboardHolder" ],
            "net/minecraft/class_1297": [ "org/wallentines/mcore/Entity" ],
            "net/minecraft/class_1799": [ "org/wallentines/mcore/ItemStack" ]
        }
    },
    "name": "MidnightCore",
    "description": "such wow.",
    "contact": {
        "homepage": "https://wallentines.org/"
    },
    "authors": [
        {
            "name": "M1dnight_Ninja"
        }
    ],
    "license": "LGPL"
}