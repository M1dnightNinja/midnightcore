{
    "schemaVersion": 1,
    "id": "${id}",
    "version": "${version}",
    "environment": "*",
    "entrypoints": {
        "main": [
          "org.wallentines.mcore.Init"
        ],
        "client": [
            "org.wallentines.mcore.InitClient"
        ]
    },
    "depends": {
        "fabricloader": "*",
        "fabric-api-base": "*"
    },
    "mixins": [
        "midnightcore.mixins.json"
    ],
    "name": "MidnightCore",
    "description": "A core library for Minecraft cross-platform plugins and mods",
    "contact": {
        "homepage": "https://github.com/wallenjos01/MidnightCore"
    },
    "authors": [
        {
            "name": "M1dnight_Ninja"
        }
    ],
    "custom": {
        "loom:injected_interfaces": {
            "net.minecraft.class_1297": [ "org/wallentines/mcore/Entity" ],
            "net.minecraft.class_1799": [ "org/wallentines/mcore/ItemStack" ],
            "net.minecraft.class_3222": [
                "org/wallentines/mcore/Player",
                "org/wallentines/mcore/ScoreboardHolder"
            ]
        }
    },
    "license": "LGPL"
}